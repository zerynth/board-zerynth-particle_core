        "cflags":["-mcpu=cortex-m3","-mthumb", "-O2","-fomit-frame-pointer","-falign-functions=16","-ffunction-sections", "-fdata-sections", "-fno-common", "-Wall", "-Wextra", "-Wstrict-prototypes","-nostdlib","-std=c99"],
        "defs":["STM32F10X_MD","STM32F1xx","STM32F103xx","STM32F103RB","VM_DEFAULT_THREAD_SIZE=1024"]
