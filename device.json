{
    "name":"Particle Core",
    "target":"particle_core",
    "cc":"armgcc",
    "classes":["pcore.ParticleCore","pcore_dfu.ParticleCoreDFU"],
    "ram":20,
    "flash":128,
    "family_type":"armcmx",
    "family_name":"stm32f1",
    "chip_model":"stm32f103",
    "manual_reset": 1,
    "preferred_baud":115200,
    "board_producer":"Particle",
    "chip_manifacturer":"STM",
    "architecture":"cortex-m3",
